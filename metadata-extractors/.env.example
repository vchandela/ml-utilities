# .env.example

# --- BigQuery Configuration ---
BIGQUERY_SA_CREDS_JSON={"type":"service_account", ...}
BIGQUERY_PROJECT_ID="your-gcp-project-id"
BIGQUERY_CLIENT_EMAIL="your-bq-service-account@your-gcp-project.iam.gserviceaccount.com"
QUERY_DAYS_BACK=800

# --- Redash Configuration ---
# The base URL of your Redash instance.
REDASH_API_URL=https://your.redash.instance
# API Key with read access to dashboards, queries, and widgets.
REDASH_API_KEY=your_redash_api_key

# --- General Configuration ---
LOG_LEVEL=INFO
