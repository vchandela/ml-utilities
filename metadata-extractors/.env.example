# .env.example

# --- BigQuery Configuration ---
BIGQUERY_SA_CREDS_JSON={"type":"service_account", ...}
BIGQUERY_PROJECT_ID="your-gcp-project-id"
BIGQUERY_CLIENT_EMAIL="your-bq-service-account@your-gcp-project.iam.gserviceaccount.com"
QUERY_DAYS_BACK=800

# --- Redash Configuration ---
REDASH_API_URL=https://your.redash.instance
REDASH_API_KEY=your_redash_api_key

# --- Bitbucket Configuration ---
# The access token obtained during the OAuth flow.
BITBUCKET_ACCESS_TOKEN=your_bitbucket_access_token
# The refresh token obtained during the OAuth flow.
BITBUCKET_REFRESH_TOKEN=your_bitbucket_refresh_token
BITBUCKET_CLIENT_ID=your_bitbucket_client_id
BITBUCKET_CLIENT_SECRET=your_bitbucket_client_secret

# Maximum number of parallel HTTP requests to Bitbucket API.
BITBUCKET_MAX_PARALLEL=20
# Directory to clone repositories into for Git operations (e.g., commit counts).
LOCAL_REPO_DIR=./cloned_repos

# --- General Configuration ---
LOG_LEVEL=INFO